   "source": [
    "def generateDeck(N=8, P=4):\n",
    "    deck = np.zeros((P*N, 2))\n",
    "    for i in range(0,P):\n",
    "        deck[i * N:i * N + N,0] = np.arange(N)\n",
    "        deck[i * N:i * N + N,1] = i\n",
    "    return deck"
   ]
